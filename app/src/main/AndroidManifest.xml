<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.demotest"
    android:versionCode="1"
    android:versionName="1.0"
    android:installLocation="auto">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
	<uses-permission android:name="android.permission.RUN_INSTRUMENTATION"/>

	<instrumentation android:name="android.test.InstrumentationTestRunner"
	    android:targetPackage="com.demo.demotest"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_functions_launcher"
        android:label="@string/app_name"
        android:name="com.demo.demotest.AppContext"
        android:theme="@style/AppTheme">
        <uses-library android:name="android.test.runner"/>
        <activity
            android:name="com.demo.demotest.activity.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.demo.demotest.LoginActivity"></activity>
        <activity android:name="com.demo.demotest.ExceptionActivity"></activity>
        <activity android:name="com.demo.demotest.ExceptionHappenActivity"></activity>
        <activity android:name="com.demo.demotest.ExceptionShowActivity"></activity>
        <receiver android:name="com.demo.demotest.receiver.ErrorHappenReceiver">
            <intent-filter >
				<action android:name="ACTION_ERROR_HAPPEN"/>
            </intent-filter>
        </receiver>
        <activity android:name="com.demo.demotest.DemicalActivity"></activity>
        <activity android:name="com.demo.demotest.ListViewItemHeightActivity"></activity>
        <activity android:name="com.demo.demotest.AlwaysToastActivity"></activity>
        <activity android:name="com.demo.demotest.ToastAndLogActivity"></activity>
        <activity android:name="com.demo.demotest.DateToSecondActivity"></activity>
    	<activity android:name="com.demo.demotest.GridViewItemHeightActivity"></activity>
    	<activity android:name="com.demo.demotest.ListViewAsyncActivity"></activity>
    	<activity android:name="com.demo.demotest.Write2XMLActivity"></activity>
    	<activity android:name="com.demo.demotest.SuperLogActivity"></activity>
    	<activity android:name="com.demo.demotest.GetTimeFromInternetActivity"></activity>
    	<activity android:name="com.demo.demotest.ViewPagerActivity"></activity>
    	<activity android:name="com.demo.demotest.HttpUtilActivity"></activity>
    	<activity android:name="com.demo.demotest.MultiThreadDownloadFileActivity"></activity>
    	<activity android:name="com.demo.demotest.ProgressBarFocusActivity"></activity>
    	<activity android:name="com.demo.demotest.LinearPressedActivity"></activity>
    	<activity android:name="com.demo.demotest.ScreenInfoActivity"></activity>
    	<activity android:name="com.demo.demotest.RefreshRotateAnimationActivity"></activity>
    	<activity android:name="com.demo.demotest.JsonTestActivity"></activity>
    	<activity android:name="com.demo.demotest.ListViewCheckBoxActivity"></activity>
    	<activity android:name="com.demo.demotest.ListViewEditTextActivity"></activity>
    	<activity android:name="com.demo.demotest.EventActivity"></activity>
    	<activity android:name="com.demo.demotest.RefreshListViewActivity"></activity>
    	<activity android:name="com.demo.demotest.ItemDeleteActivity"></activity>
    	<activity android:name="com.demo.demotest.ListViewRollActivity"></activity>
    	<activity android:name="com.demo.demotest.SDPathTestActivity"></activity>
    	<activity android:name="com.demo.demotest.InternetAvailableActivity"></activity>
    	<activity android:name="com.demo.demotest.GetAllActivitiesActivity"></activity>
    	<activity android:name="com.demo.demotest.HTMLParseActivity"></activity>
    	<activity android:name="com.demo.demotest.ExpandableListViewActivity"></activity>
    	<activity android:name="com.demo.demotest.PaginationListViewActivity"></activity>
    	<activity android:name="com.demo.demotest.VideoPlayerActivity"></activity>
    	<activity android:name="com.demo.demotest.SurfaceViewActivity"></activity>
    	<activity android:name="com.demo.demotest.TabHostActivity"></activity>
    	<activity android:name="com.demo.demotest.ui.ClickSpanTextViewActivity"></activity>
    	<activity android:name="com.demo.demotest.GridViewMenuActivity"></activity>
    	<activity android:name="com.demo.demotest.activity.CalendarActivity"></activity>
    	<activity android:name="com.demo.demotest.activity.ItemRefreshActivity"></activity>
    	<activity android:name="com.demo.demotest.activity.ImageViewSizeAutoActivity"></activity>
    	<activity android:name="com.demo.demotest.activity.EditTextDrawableActivity"></activity>
